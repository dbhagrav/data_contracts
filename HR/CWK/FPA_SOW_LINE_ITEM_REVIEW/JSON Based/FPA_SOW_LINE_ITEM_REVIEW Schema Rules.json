{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "INVOICE_ID": {
      "anyOf": [
        {
          "type": [ "null" ]
        },
        {
          "pattern": "^([1-9]\\d*)$"
        }
      ],
      "description": "Can be null, or an integer value greater than zero."
    }
  }
}
