{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "PRODUCT_NAME": {
      "allOf": [
        { "type": "string" },
        { "not": { "type": "null" } },
        { "pattern": "^[1-9]\\d*$" }
      ],
      "description": "Cannot be null, and must be an integer value greater than zero."
    }
  }
}
